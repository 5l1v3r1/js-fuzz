{
  "name": "js-fuzz",
  "version": "0.1.0",
  "description": "An AFL-inspired genetic fuzz tester for JavaScript",
  "main": "index.js",
  "scripts": {
    "build:dist": "npm run -s clean && tsc",
    "clean": "rm -rf coverage doc lib",
    "test": "npm-run-all --parallel test:lint test:unit test:integrate",
    "test:lint": "tslint --type-check --project tsconfig.json '{src,test}/**/*.ts'",
    "test:unit": "mocha --compilers ts:ts-node/register test/unit/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/connor4312/js-fuzz.git"
  },
  "author": "Connor Peet <connor@peet.io>",
  "license": "MIT",
  "dependencies": {
    "clone": "^2.1.0",
    "istanbul": "^0.4.5",
    "rxjs": "^5.0.3",
    "yargs": "^6.6.0"
  },
  "bin": {
    "js-fuzz": "./bin/js-fuzz"
  },
  "devDependencies": {
    "@types/clone": "^0.1.30",
    "@types/node": "^7.0.0",
    "npm-run-all": "^4.0.0",
    "standard": "^8.6.0",
    "ts-node": "^2.0.0",
    "tslint": "^4.3.1",
    "tslint-microsoft-contrib": "^4.0.0",
    "typescript": "^2.1.5"
  },
  "bugs": {
    "url": "https://github.com/connor4312/js-fuzz/issues"
  },
  "homepage": "https://github.com/connor4312/js-fuzz#readme",
  "keywords": [
    "afl",
    "american",
    "fuzzy",
    "lop",
    "fuzzer",
    "tester",
    "fuzz"
  ]
}
